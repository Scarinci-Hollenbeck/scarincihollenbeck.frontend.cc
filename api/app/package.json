{
  "name": "scarincihollenbeck-worker",
  "version": "1.0.0",
  "description": "worker to fetch data from admin.scarincihollenbeck.com",
  "main": "./src/api/server.ts",
  "scripts": {
    "start": "node .",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly src/api/server.ts",
    "prod": "tsc && node ./www/api/server.js",
    "clean": "rm -rf www/ || true",
    "build": "npm run clean && tsc && cp .npmrc www/.npmrc && cp package.json www/package.json",
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "worker": "tsc && node ./www/run-workers.js",
    "hello": "node src/hello.js"
  },
  "author": "peter t",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "custom-env": "^2.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mocha": "^7.0.1",
    "node-fetch": "^2.6.0",
    "redis": "^2.8.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.9",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.1.8",
    "@types/node-fetch": "^2.5.4",
    "@types/redis": "^2.8.17",
    "@types/supertest": "^2.0.8",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  }
}
