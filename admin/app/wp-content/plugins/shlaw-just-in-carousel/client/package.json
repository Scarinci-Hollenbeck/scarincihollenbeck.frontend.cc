{
  "name": "simple_webpack_boilerplate",
  "version": "1.0.0",
  "description": "A ready to use simple webpack boilerplate",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --env.ENVIRONMENT=development --config webpack.config.js --watch",
    "build-development": "webpack --mode production --env.ENVIRONMENT=development --config webpack.config.js",
    "build-staging": "webpack --mode production --env.ENVIRONMENT=staging --config webpack.config.js",
    "build-production": "webpack --mode production --env.ENVIRONMENT=production --config webpack.config.js",
    "production": "cp ./dist/main.js ../includes/assets/js/just-in.min.js && cp ./dist/main.css ../includes/assets/css/just-in.css"
  },
  "author": "peter tumulty",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "extract-css-chunks-webpack-plugin": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "es6-promise": "^4.2.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.0.2",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-icons": "^3.2.2",
    "react-redux": "^6.0.0",
    "react-slick": "^0.23.2",
    "react-spinners": "^0.4.8",
    "redux": "^4.0.1",
    "sass-loader": "^8.0.0",
    "slick-carousel": "^1.8.1"
  }
}
